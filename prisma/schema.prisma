generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Candidato {
  id              String   @id @default(uuid())
  primeiro_nome   String
  segundo_nome    String
  cpf             String   @unique
  data_nascimento DateTime
  senha           String
}
